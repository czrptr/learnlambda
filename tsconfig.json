{
	"compilerOptions": {
	"types": [
		"node",
		"jest"
	],
	"target": "ES6",
	"module": "commonjs",
	"lib": ["dom", "es2015"],
	"checkJs": false,
	"sourceMap": true,
	"outDir": "./out",
	"rootDir": "./src",
	"downlevelIteration": true,
	"strict": true,
	"strictNullChecks": true,
	"noImplicitThis": true,
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"./src/tests/**/*"
	]
}